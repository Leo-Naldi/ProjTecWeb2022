{
  "name": "backend",
  "version": "1.0.0",
  "description": "Database API for AnimalHouse",
  "main": "run.js",
  "scripts": {
    "pretest": "clear && npm run remake-db",
    "test": "mocha",
    "start": "node run.js",
    "start-dev": "nodemon -e js,json run.js",
    "make-db": "find ./data/setup_data -name '*.js' -print0 | xargs -0 mongosh -f",
    "remake-db": "mongosh -f ./data/drop_all.js  && npm run make-db"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.2",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.2.0",
    "mongodb": "^4.13.0",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "supertest": "^6.3.3"
  }
}
